<ProfileHeader @profile={{this.profile}} @title="2010 Census Profile" @mode={{this.profile.mode}} @data={{this.rawData}}>
</ProfileHeader>

<hr>

<h3 id="density" class="header-medium">Population Density</h3>
<div class="grid-x grid-margin-x">
  <div class="cell large-auto">
    {{data-table
      decennial=true
      reliability=false
      mode=this.profile.mode
      comparison=true
      config=this.populationDensity
      model=this.model
    }}
  </div>
</div>

<hr>

