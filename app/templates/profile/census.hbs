{{#profile-header
  profile=profile
  title="2010 Census Profile"
  mode=profile.mode
  data=rawData
}}
{{/profile-header}}

<hr>

<h3 id="sex-and-age" class="header-medium">Population Density</h3>
<div class="grid-x grid-margin-x">
  <div class="cell large-8">
    {{#decennial-table taskInstance=model.population_density as |table|}}
      {{data-table
        decennial=true
        reliability=false
        mode=profile.mode
        comparison=true
        config=popDensity
        t1=table.taskInstance.y2010
        t2=table.taskInstance.y2000
      }}
    {{/decennial-table}}
  </div>
</div>

<hr>

