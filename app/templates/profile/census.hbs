<h3 id="sex-and-age" class="header-medium">Sex and Age</h3>
<div class="grid-x grid-margin-x">
  <div class="cell large-8">
    {{#decennial-table category='decennial_sex_age' as |table|}}
      {{acs-table
        reliability=false
        comparison=true
        config=sexAge
        t1=table.categoryData.y2000
        t2=table.categoryData.y2010
      }}
    {{/decennial-table}}
  </div>
</div>

