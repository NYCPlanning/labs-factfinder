{{#profile-header
  profile=profile
  title="2010 Census Profile"
  mode=profile.mode
  data=rawData
}}
{{/profile-header}}

<hr>

<h3 id="density" class="header-medium">Population Density</h3>
<div class="grid-x grid-margin-x">
  <div class="cell large-auto">
    {{data-table
      decennial=true
      reliability=false
      mode=profile.mode
      comparison=true
      config=populationDensity
      model=model
    }}
  </div>
</div>

<hr>

