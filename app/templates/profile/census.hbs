{{#profile-header
  profile=profile
  title="2010 Census Profile"
  mode=profile.mode
  data=rawData
}}
{{/profile-header}}

<hr>

<h3 id="density" class="header-medium">Population Density</h3>
<div class="grid-x grid-margin-x">
  <div class="cell large-auto">
    {{data-table
      decennial=true
      reliability=false
      mode=profile.mode
      comparison=true
      config=populationDensity
      t1=model.y2010
      t2=model.y2000
    }}
  </div>
</div>

<hr>

