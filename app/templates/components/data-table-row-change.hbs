{{#unless (or @rowConfig.divider this.noPriorData)}}
  <td class='title-column' {{action 'showData'}}>
    <span>
      {{@rowConfig.title}}
      {{#if @rowConfig.tooltip}}
        {{fa-icon icon='question-circle' transform='shrink-2'}}
        {{#if (eq this.rowConfig.title 'Median earnings for workers (dollars)')}}
          {{ember-tooltip text=this.rowConfig.tooltip side='right'}}
        {{else}}
          {{ember-tooltip text=this.rowConfig.tooltip side='top-start'}}
        {{/if}}
      {{/if}}
    </span>
  </td>

  {{!-- OLD --}}
  {{data-table-column-group
    model=@data.previous
    rowConfig=@rowConfig
    reliabilityMode=@reliability}}

  {{!-- CURRENT --}}
   {{data-table-column-group
    model=@data.selection
    rowConfig=@rowConfig
    reliabilityMode=@reliability}}

  {{!-- CHANGE --}}

  {{!-- Estimate --}}
  {{#with (format-number @data.changeSum precision=@rowConfig.decimal) as |roundedChangeSum|}}
    {{#with (format-number @data.changeMarginOfError precision=@rowConfig.decimal) as |roundedChangeMarginOfError|}}
      <td class="cell-border-left change-estimate {{unless @data.changeSignificant 'insignificant'}}">
        {{#unless (eq @data.changeSum null)}}

          {{roundedChangeSum}}

          {{#if (and
            (lte @data.changeMarginOfError (abs @data.changeSum))
            (not (or
              (eq @data.previous.sum 0)
              (eq @data.selection.sum 0)
            ))
            (not (eq
              roundedChangeSum
              roundedChangeMarginOfError
            ))
          )}}
            {{#if (gt @data.changeSum 0)}}
              {{fa-icon icon="arrow-up" class="green"}}
            {{else if (lt @data.changeSum 0)}}
              {{fa-icon icon="arrow-down" class="red"}}
            {{else}}
              {{fa-icon icon=""}}
            {{/if}}
          {{/if}}
        {{/unless}}
      </td>

      {{#if @reliability}}
        {{!-- MOE --}}
        <td class="{{unless @data.changeSignificant 'insignificant'}} change-estimate-moe">
          {{unless (eq @data.changeMarginOfError null) roundedChangeMarginOfError}}
        </td>
      {{/if}}
    {{/with}}
  {{/with}}

  {{!-- percent --}}
  {{#with (format-number (mult @data.changePercent 100) precision=1) as |roundedChangePercentage|}}
    {{#with (format-number (mult @data.changePercentMarginOfError 100) precision=1) as |roundedChangePercentageMarginOfError|}}

      <td class="{{unless @data.changePercentSignificant 'insignificant'}} change-percent">
        {{#unless (eq @data.changePercent null)}}

          {{concat
            roundedChangePercentage
            '%'
          }}

          {{#if (and
            (lte @data.changePercentMarginOfError (abs @data.changePercent))
            (not (or
              (eq @data.previous.sum 0)
              (eq @data.selection.sum 0)
            ))
            (not (eq
              roundedChangePercentage
              roundedChangePercentageMarginOfError
            ))
          )}}
            {{#if (gt @data.changePercent 0)}}
              {{fa-icon icon="arrow-up" class="green"}}
            {{else if (lt @data.changePercent 0)}}
              {{fa-icon icon="arrow-down" class="red"}}
            {{else}}
              {{fa-icon icon=""}}
            {{/if}}
          {{/if}}
        {{/unless}}
      </td>

      {{#if @reliability}}
        {{!-- Percent M --}}
        <td class="{{unless @data.changePercentSignificant 'insignificant'}} change-percent-moe">
          {{unless (eq @data.changePercentMarginOfError null)
            (concat
              roundedChangePercentageMarginOfError
              '%'
            )
          }}
        </td>
      {{/if}}
    {{/with}}
  {{/with}}

  {{!-- percentage point --}}

  {{#with (format-number
    (mult @data.changePercentagePoint 100)
    precision=1) as |roundedChangePercentagePoint|}}
    {{#with (format-number
      (mult @data.changePercentagePointMarginOfError 100)
      precision=1) as |roundedChangePercentagePointMarginOfError|}}

      <td class="{{unless @data.changePercentagePointSignificant 'insignificant'}} change-percentage-point">
        {{#unless
          (or
            @data.isSpecial
            (eq @data.changePercentagePoint null)
          )
        }}

          {{roundedChangePercentagePoint}}

          {{#if (and
            (lte @data.changePercentagePointMarginOfError (abs @data.changePercentagePoint))
            (not (or
              (eq @data.previous.sum 0)
              (eq @data.selection.sum 0)
            ))
            (not (eq
              roundedChangePercentagePoint
              roundedChangePercentagePointMarginOfError
            ))
          )}}
            {{#if (gt @data.changePercentagePoint 0)}}
              {{fa-icon icon="arrow-up" class="green"}}
            {{else if (lt @data.changePercentagePoint 0)}}
              {{fa-icon icon="arrow-down" class="red"}}
            {{else}}
              {{fa-icon icon=""}}
            {{/if}}
          {{/if}}
        {{/unless}}
      </td>

      {{#if @reliability}}
        {{!-- percentage point m --}}
        <td class="{{unless @data.changePercentagePointSignificant 'insignificant'}} change-percentage-point-moe">
          {{#unless
              (or
                @data.isSpecial
                (eq @data.changePercentagePointMarginOfError null)
              )}}

            {{roundedChangePercentagePointMarginOfError}}

          {{/unless}}
        </td>
      {{/if}}
    {{/with}}
  {{/with}}
{{else}}
  {{#if this.noPriorData}}
    {{!-- if 2006-2010 sum is null, just return empty cells --}}
    <td class='title-column' {{action 'showData'}}>
      <span>
        {{@rowConfig.title}}
        {{#if @rowConfig.tooltip}}
          {{fa-icon icon='question-circle' transform='shrink-2'}}
          {{ember-tooltip text=@rowConfig.tooltip side='right'}}
        {{/if}}
      </span>
    </td>
    <td class="cell-border-left" class="no-compare-message">
      <div>Data for this row are not comparable or are unavailable</div>
    </td>
  {{else}}
    <td>&nbsp;</td>
    <td class="cell-border-left">&nbsp;</td>
  {{/if}}
  <td>&nbsp;</td>
  {{#if @reliability}}<td>&nbsp;</td>{{/if}}
  {{#if @reliability}}<td>&nbsp;</td>{{/if}}
  {{#if @reliability}}<td>&nbsp;</td>{{/if}}

  <td class="cell-border-left">&nbsp;</td>
  <td>&nbsp;</td>
  {{#if @reliability}}<td>&nbsp;</td>{{/if}}
  {{#if @reliability}}<td>&nbsp;</td>{{/if}}
  {{#if @reliability}}<td>&nbsp;</td>{{/if}}

  <td class="cell-border-left">&nbsp;</td>
  <td>&nbsp;</td>
  <td>&nbsp;</td>
  {{#if @reliability}}<td>&nbsp;</td>{{/if}}
  {{#if @reliability}}<td>&nbsp;</td>{{/if}}
  {{#if @reliability}}<td>&nbsp;</td>{{/if}}
{{/unless}}

{{yield}}
