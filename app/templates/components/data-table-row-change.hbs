{{#unless (or @rowConfig.divider this.noPriorData)}}
  <td class='title-column' {{action 'showData'}}>
    <span>
      {{@rowConfig.title}}
      {{#if @rowConfig.tooltip}}
        {{fa-icon icon='question-circle' transform='shrink-2'}}
        {{ember-tooltip text=@rowConfig.tooltip side='right'}}
      {{/if}}
    </span>
  </td>

  {{!-- OLD --}}
  {{data-table-column-group
    model=@data.previous
    rowConfig=@rowConfig
    reliabilityMode=@reliability}}

  {{!-- CURRENT --}}
   {{data-table-column-group
    model=@data.selection
    rowConfig=@rowConfig
    reliabilityMode=@reliability}}

  {{!-- CHANGE --}}

  {{!-- Estimate --}}
  <td class="cell-border-left change-estimate {{unless @data.changeSignificant 'insignificant'}}">
    {{#unless (eq @data.changeSum null)}}
      {{format-number
        @data.changeSum
        precision=@rowConfig.decimal
      }}

      {{#if (and
        (gt @data.changeSum 0)
        (lt @data.changeMarginOfError @data.changeSum)
      )}}
        {{fa-icon icon="arrow-up" class="green"}}
      {{else if (and
        (lt @data.changeSum 0)
        (gt @data.changeMarginOfError @data.changeSum)
      )}}
        {{fa-icon icon="arrow-down" class="red"}}
      {{else}}
        {{fa-icon icon=""}}
      {{/if}}
    {{/unless}}
  </td>

  {{#if @reliability}}
    {{!-- MOE --}}
    <td class="{{unless @data.changeSignificant 'insignificant'}} change-estimate-moe">
      {{unless (eq @data.changeMarginOfError null)
        (format-number
            @data.changeMarginOfError
            precision=@rowConfig.decimal)}}
    </td>
  {{/if}}

  {{!-- percent --}}
  <td class="{{unless @data.changePercentSignificant 'insignificant'}} change-percent">
    {{#unless (eq @data.changePercent null)}}
      {{concat
        (format-number (mult @data.changePercent 100) precision=1)
        '%'
      }}

      {{#if (and
        (gt @data.changePercent 0)
        (lt @data.changePercentMarginOfError @data.changePercent)
      )}}
        {{fa-icon icon="arrow-up" class="green"}}
      {{else if (and
        (lt @data.changePercent 0)
        (gt @data.changePercentMarginOfError @data.changePercent)
      )}}
        {{fa-icon icon="arrow-down" class="red"}}
      {{else}}
        {{fa-icon icon=""}}
      {{/if}}
    {{/unless}}
  </td>

  {{#if @reliability}}
    {{!-- Percent M --}}
    <td class="{{unless @data.changePercentSignificant 'insignificant'}} change-percent-moe">
      {{unless (eq @data.changePercentMarginOfError null)
        (concat
          (format-number (mult @data.changePercentMarginOfError 100) precision=1)
          '%'
        )
      }}
    </td>
  {{/if}}

  {{!-- percentage point --}}
  <td class="{{unless @data.changePercentagePointSignificant 'insignificant'}} change-percentage-point">
    {{#unless
      (or
        @data.isSpecial
        (eq @data.changePercentagePoint null)
      )
    }}
      {{format-number
        (mult @data.changePercentagePoint 100)
        precision=1}}

      {{#if (and
        (gt @data.changePercentagePoint 0)
        (lt @data.changePercentagePointMarginOfError @data.changePercentagePoint)
      )}}
        {{fa-icon icon="arrow-up" class="green"}}
      {{else if (and
        (lt @data.changePercentagePoint 0)
        (gt @data.changePercentagePointMarginOfError @data.changePercentagePoint)
      )}}
        {{fa-icon icon="arrow-down" class="red"}}
      {{else}}
        {{fa-icon icon=""}}
      {{/if}}
    {{/unless}}
  </td>

  {{#if @reliability}}
    {{!-- percentage point m --}}
    <td class="{{unless @data.changePercentagePointSignificant 'insignificant'}} change-percentage-point-moe">
      {{#unless
          (or
            @data.isSpecial
            (eq @data.changePercentagePointMarginOfError null)
          )}}
        {{format-number
          (mult @data.changePercentagePointMarginOfError 100)
          precision=1}}
      {{/unless}}
    </td>
  {{/if}}
{{else}}
  {{#if this.noPriorData}}
    {{!-- if 2006-2010 sum is null, just return empty cells --}}
    <td class='title-column' {{action 'showData'}}>
      <span>
        {{@rowConfig.title}}
        {{#if @rowConfig.tooltip}}
          {{fa-icon icon='question-circle' transform='shrink-2'}}
          {{ember-tooltip text=@rowConfig.tooltip side='right'}}
        {{/if}}
      </span>
    </td>
    <td class="cell-border-left" class="no-compare-message">
      <div>Data for this row are not comparable or are unavailable</div>
    </td>
  {{else}}
    <td>&nbsp;</td>
    <td class="cell-border-left">&nbsp;</td>
  {{/if}}
  <td>&nbsp;</td>
  {{#if @reliability}}<td>&nbsp;</td>{{/if}}
  {{#if @reliability}}<td>&nbsp;</td>{{/if}}
  {{#if @reliability}}<td>&nbsp;</td>{{/if}}

  <td class="cell-border-left">&nbsp;</td>
  <td>&nbsp;</td>
  {{#if @reliability}}<td>&nbsp;</td>{{/if}}
  {{#if @reliability}}<td>&nbsp;</td>{{/if}}
  {{#if @reliability}}<td>&nbsp;</td>{{/if}}

  <td class="cell-border-left">&nbsp;</td>
  <td>&nbsp;</td>
  <td>&nbsp;</td>
  {{#if @reliability}}<td>&nbsp;</td>{{/if}}
  {{#if @reliability}}<td>&nbsp;</td>{{/if}}
  {{#if @reliability}}<td>&nbsp;</td>{{/if}}
{{/unless}}

{{yield}}
