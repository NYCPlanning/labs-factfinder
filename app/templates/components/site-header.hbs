<header role="banner">
  <a href="https://planninglabs.nyc/" class="labs-beta-notice hide-for-print">A beta project of NYC Planning Labs</a>
  <div class="grid-x grid-padding-x">
    <div class="branding cell shrink large-auto">
      <a class="dcp-link" href="http://www1.nyc.gov/site/planning/index.page"><img class="dcp-logo" src="https://raw.githubusercontent.com/NYCPlanning/logo/master/dcp_logo_772.png" alt="NYC Planning"></a>
      {{#link-to 'index' classNames='site-name'}}Population FactFinder{{/link-to}}
    </div>
    <div class="cell auto hide-for-large text-right">
      <button {{action (mut closed) (not closed)}} class="responsive-nav-toggler hide-for-print" data-toggle="responsive-menu">Menu</button>
    </div>
    <div id="responsive-menu" class="cell large-shrink {{if closed 'show-for-large'}} hide-for-print" data-toggler=".show-for-large">
      <nav role="navigation">
        <ul class="vertical large-horizontal menu">
          <li>{{link-to 'About' 'about' invokeAction=(action (mut closed))}}</li>
          <li>{{link-to 'Tutorial' 'tutorial' invokeAction=(action (mut closed))}}</li>
          <li>{{link-to 'Features' 'features' invokeAction=(action (mut closed))}}</li>
          <li>{{link-to 'Data' 'data' invokeAction=(action (mut closed))}}</li>
          <li><a href="mailto:pff_dl@planning.nyc.gov?subject=Population%20FactFinder%20Feedback">{{fa-icon "envelope"}}</a></li>
        </ul>
      </nav>
    </div>
  </div>
</header>
{{yield}}
