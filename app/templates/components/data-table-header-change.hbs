<table class="header-table">
  <thead>
    <tr>
      <th rowspan="{{if reliability "3" "2"}}">
        {{#unless decennial}}
          <div class="profile-checkbox-controls">
            <a {{action (mut reliability) (not reliability)}}>
              {{#if reliability}}
                <span class="dcp-orange">{{fa-icon 'check-square' fixedWidth=true}}</span>
              {{else}}
                <span class="dcp-orange">{{fa-icon 'square' fixedWidth=true}}</span>
              {{/if}}
              Show Reliability Data
            </a>
          </div>
        {{/unless}}
      </th>
      {{#if decennial}}
      <th colspan="{{if reliability "5" "2"}}" class="text-center cell-border-bottom">2000</th>
      <th colspan="{{if reliability "5" "2"}}" class="text-center cell-border-bottom">2010</th>
        <th colspan="3" class="text-center cell-border-bottom">Change, 2000 to 2010</th>
      {{else}}{{!-- acs --}}
        <th colspan="{{if reliability "5" "2"}}" class="text-center cell-border-bottom">2006-2010</th>
        <th colspan="{{if reliability "5" "2"}}" class="text-center cell-border-bottom">2012-2016</th>
        <th colspan="{{if reliability "6" "3"}}" class="text-center cell-border-bottom">Change, 2006-2010 to 2012-2016</th>
      {{/if}}
    </tr>
    {{#if reliability}}
    <tr>
      <th colspan="3" class="text-center cell-border-bottom">Number</th>
      <th colspan="2" class="text-center cell-border-bottom">Percent</th>

      <th colspan="3" class="text-center cell-border-bottom">Number</th>
      <th colspan="2" class="text-center cell-border-bottom">Percent</th>

      <th colspan="2" class="text-center cell-border-bottom">Number</th>
      <th colspan="2" class="text-center cell-border-bottom">Percent {{info-tooltip tip='2012-2016 number minus 2006-2010 number, divided by 2006-2010 number. Quotient multiplied by 100.'}}</th>
      <th colspan="2" class="text-center cell-border-bottom">Pctg. Pt.</th>
    </tr>
    <tr>
      <th class="text-right cell-border-bottom">Estimate</th>
      <th class="text-right cell-border-bottom">MOE {{info-tooltip tip='Margin of error'}}</th>
      <th class="text-right cell-border-bottom">CV {{info-tooltip tip='Coefficient of variation'}}</th>
      <th class="text-right cell-border-bottom">Estimate</th>
      <th class="text-right cell-border-bottom">MOE {{info-tooltip tip='Margin of error'}}</th>

      <th class="text-right cell-border-bottom">Estimate</th>
      <th class="text-right cell-border-bottom">MOE {{info-tooltip tip='Margin of error'}}</th>
      <th class="text-right cell-border-bottom">CV {{info-tooltip tip='Coefficient of variation'}}</th>
      <th class="text-right cell-border-bottom">Estimate</th>
      <th class="text-right cell-border-bottom">MOE {{info-tooltip tip='Margin of error'}}</th>

      <th class="text-right cell-border-bottom">Estimate</th>
      <th class="text-right cell-border-bottom">MOE {{info-tooltip tip='Margin of error'}}</th>
      <th class="text-right cell-border-bottom">Estimate</th>
      <th class="text-right cell-border-bottom">MOE {{info-tooltip tip='Margin of error'}}</th>
      <th class="text-right cell-border-bottom">Estimate</th>
      <th class="text-right cell-border-bottom">MOE {{info-tooltip tip='Margin of error'}}</th>
    </tr>
    {{else}}
    <tr>
      <th class="text-right cell-border-bottom">Number</th>
      <th class="text-right cell-border-bottom">Percent</th>
      <th class="text-right cell-border-bottom">Number</th>
      <th class="text-right cell-border-bottom">Percent</th>
      <th class="text-right cell-border-bottom">Number</th>
      <th class="text-right cell-border-bottom">Percent {{info-tooltip tip='2010 number minus 2000 number, divided by 2000 number. Quotient multiplied by 100.'}}</th>
      <th class="text-right cell-border-bottom">Pctg. Pt.</th>
    </tr>
    {{/if}}
  </thead>
</table>
{{yield}}
