{{#unless rowconfig.divider}}
  <td class='title-column'>
    {{rowconfig.title}}
    {{#if rowconfig.tooltip}}
      {{info-tooltip tip=rowconfig.tooltip}}
    {{/if}}
  </td>

  {{!-- SELECTED AREA --}}
  {{data-table-column-group
    model=data.selection
    rowConfig=rowConfig
    reliabilityMode=reliability}}

  {{!-- COMPARISON AREA --}}
  {{data-table-column-group
    model=data.comparison
    rowConfig=rowConfig
    reliabilityMode=reliability}}

  {{!-- DIFFERENCE --}}
  <td 
    class="cell-border-left 
    {{unless data.significant 'insignificant'}}">
    {{format-number data.difference_sum precision=rowconfig.decimal}}
  </td>
  {{#if reliability}}
    <td>
      {{data.differenceM}}
    </td>
  {{/if}}
  <td 
    class="
    {{unless data.percent_significant 'insignificant'}}">
    {{data.difference_percent}}
  </td>
  {{#if reliability}}
    <td>{{data.difference_percent_m}}</td>
  {{/if}}

{{else}}

  <td>&nbsp;</td>
  <td class="cell-border-left">&nbsp;</td>
  <td>&nbsp;</td>
  {{#if reliability}}
    <td>&nbsp;</td>
    <td>&nbsp;</td>
    <td>&nbsp;</td>
  {{/if}}

  <td class="cell-border-left">&nbsp;</td>
  <td>&nbsp;</td>
  {{#if reliability}}
    <td>&nbsp;</td>
    <td>&nbsp;</td>
    <td>&nbsp;</td>
  {{/if}}

  <td class="cell-border-left">&nbsp;</td>
  <td>&nbsp;</td>
  {{#if reliability}}
    <td>&nbsp;</td>
    <td>&nbsp;</td>
  {{/if}}

{{/unless}}
{{yield}}
